extends ./base/layout.pug

block content
    include _header_static.pug
    .wrapper
        section.pd_top--x100.pd_bottom--x100
            .container
                .w-50.mx-auto
                    .txt_center.py-5
                        .title.mb-4(data-aos="fade-up")
                            .icon.icon_hi

                .w-75.mx-auto
                    .txt_center(data-aos="fade-up")
                        .title.mb-4
                            h2 Ingresa a tu casillero virtual
            .container.pd_bottom--x100.pd_top--x100
                .w-50.mx-auto.bgc--white.box_shadow.txt_center.pd_box--x50(data-aos="flip-left", data-aos-duration="3000")
                    | iframe
    include _footer.pug